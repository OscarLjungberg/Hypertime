
pipeline {
    agent any
    
    tools {nodejs "node"}

    stages {
        stage('install dependencies') {
            steps {
                sh ' cd hypertime-frontend && npm install && cd ..'
            }
        }
        stage('Build dist files') {
            steps {
                sh ' cd hypertime-frontend && npm run build && cd ..'
            }
        }
        stage('test etc') {
            parallel {
                stage('Test') {
                    steps {
                        sh ' cd hypertime-frontend && npm run test && cd ..'
                    }
                }
                stage('test 2') {
                    steps {
                        sh 'echo test'
                    }
                }

            }
        }
        }
    }
}