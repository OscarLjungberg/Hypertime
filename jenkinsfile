
pipeline {
    agent any
    
    tools {nodejs "node"}

    stages {
        stage('Build echo') {
            steps {
                sh "echo \"${env.WORKSPACE}\"/hypertime-frontend/build"
            }
        }
        stage('Deploy') {
            steps {
                sh "az storage blob upload-batch -s  \"${env.WORKSPACE}\"/hypertime-frontend/build -d %24web --account-name hypertime --connection-string=DefaultEndpointsProtocol=https;AccountName=hypertime;AccountKey=/hKquqMB4jLoma30s9HdVXJhiCIf++3loLZQe2nz0RPjPXiMs7xd8B96vFGsvr8lPVpMMVyp7rbBXBQ9+R+XYA==;EndpointSuffix=core.windows.net"
            }
        }
        
    }
}